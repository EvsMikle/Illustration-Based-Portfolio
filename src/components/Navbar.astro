---
// Define your navigation links here
const navLinks = [
  { href: '/', label: 'Home' },
  { href: '/#about', label: 'About' },
  { href: '/#projects', label: 'Projects' },
  { href: '/#skills', label: 'Skills' },
  { href: '/#contact', label: 'Contact' },
];
---
<div class="w-[1440px] h-[104px] px-20 py-6 justify-start items-start inline-flex">
  <div class="h-14 px-8 justify-between items-center flex">
      <div class="justify-start items-center gap-3 flex">
          <div class="w-[39.91px] h-10 relative">
              <div class="w-[23.15px] h-[34.73px] left-[8.38px] top-[2.64px] absolute">
              </div>
          </div>
          <div class="text-black text-xl font-bold font-['Sora'] capitalize leading-normal">Personal</div>
      </div>
      <div class="justify-center items-center gap-8 flex">
          <div class="justify-center items-center gap-2.5 flex">
              <div class="text-black text-xl font-semibold font-['Sora'] capitalize leading-normal">About Me</div>
          </div>
          <div class="justify-center items-center gap-2.5 flex">
              <div class="text-black text-xl font-semibold font-['Sora'] capitalize leading-normal">Skills</div>
          </div>
          <div class="justify-center items-center gap-2.5 flex">
              <div class="text-black text-xl font-semibold font-['Sora'] capitalize leading-normal">Project</div>
          </div>
          <div class="justify-center items-center gap-2.5 flex">
              <div class="text-black text-xl font-semibold font-['Sora'] capitalize leading-normal">Contact me</div>
          </div>
      </div>
      <div class="w-[153px] px-5 py-4 bg-black rounded justify-center items-center gap-2 flex">
          <div class="text-white text-xl font-semibold font-['Sora'] leading-normal tracking-tight">Resume</div>
          <div class="w-5 h-5 relative"></div>
      </div>
  </div>
</div>

<nav class="container mx-auto">
    <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
      <a href="#" class="flex items-center space-x-3 rtl:space-x-reverse">
          <img src="/logo-2.svg" class="h-8" alt="Flowbite Logo" /><p class="text-black text-xl font-bold bg-blue-700">Personal</p>
      </a>
      <button data-collapse-toggle="navbar-default" type="button" class="inline-flex items-center p-2 w-10 h-10 justify-center text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 " aria-controls="navbar-default" aria-expanded="false">
          <span class="sr-only">Open main menu</span>
          <svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 17 14">
              <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 1h15M1 7h15M1 13h15"/>
          </svg>
      </button>
      <div class="hidden w-full md:block md:w-auto" id="navbar-default">
        <ul class="font-medium flex flex-col p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:flex-row md:space-x-8 rtl:space-x-reverse md:mt-0 md:border-0 md:bg-white ">
        {navLinks.map(link => (
            <li>
                <a href={link.href} class="block py-2 px-3 text-black bg-blue-700 rounded md:bg-transparent md:text-blue-700 md:p-0">{link.label}</a>
            </li>
            ))}
        </ul>
        <div class="w-[153px] h-14 px-5 py-4 bg-black rounded justify-center items-center gap-2 inline-flex">
          <div class="text-white text-xl font-semibold font-['Sora'] leading-normal tracking-tight">Resume</div>
          <div class="w-5 h-5 relative"></div>
        </div>
      </div>
    </div>
</nav>
<script>
    document.addEventListener('DOMContentLoaded', (event) => {
        const toggleButton = document.querySelector('[data-collapse-toggle="navbar-default"]');
        const navbar = document.getElementById('navbar-default');
    
        toggleButton.addEventListener('click', () => {
            navbar.classList.toggle('hidden');
        });
    });
</script>